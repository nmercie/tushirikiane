function hidemenu(){$("ul #hme").removeClass("selected");$("ul #training").removeClass("selected");$("ul #instructr").removeClass("selected");$("ul #trngpartner").removeClass("selected");$("ul #store").removeClass("selected");$("ul #ciso").removeClass("selected");$("ul #about").removeClass("selected")}function LLbook(n,t){return window.open("/Training/LLDownload?ebookFormat="+n+"&ModuleId="+t,"_blank"),!1}function downloadbook(n,t){return window.open("/Training/EbookDownload?ebookFormat="+n+"&ModuleId="+t,"_blank"),!1}function downloadLabMan(n,t){return window.open("/Training/DownloadLabManual?LabName="+n+"&ModuleId="+t,"_blank"),!1}function downloadtools(n){return window.location.href="/Training/DownloadTools?Serial="+n,!1}function launchlab(n){window.open("/Training/GetLabLaunchURL?ilabsProfileID="+n,"_blank")}function GetCyberQLaunchURL(n,t){window.open("/Training/GetCyberQLaunchURL?CyberQLabProfileID="+n+"&a="+t,"_blank")}$(function(){$("#HFIsAcad").val()=="True"?$('.nav-tabs a[href="#acc_9_1"]').tab("show"):$('.nav-tabs a[href="#Instructions"]').tab("show")});$(document).ready(function(){hidemenu();$("ul #mycourse").addClass("selected");$("#btnValidateProctorSetupLab").click(function(){$.ajax({type:"POST",url:"/Training/SetLabEnvironment",contentType:"application/json;charset=utf-8",dataType:"json",success:function(n){n==1?$("#PRangeValidations").html('<div class="alert alert-success with-icon"><i class="fa fa-check"><\/i> Lab environment setup has been completed successfully. <\/div>'):n==2?$("#PRangeValidations").html('<div class="alert alert-danger with-icon"><i class="fa fa-times"><\/i> Sorry you do not have access to this resource.<\/div>'):n==3?$("#PRangeValidations").html('<div class="alert alert-danger with-icon"><i class="fa fa-times"><\/i> You have already setup the lab environment.<\/div>'):window.location.href="/Home/Oops"}})})});var iid;$(".itag").on("click",function(){iid=$(this).data("idtag");$("#accid").val("#"+iid);var n=$(this).data("id");window.scrollTo(0,100)});$(".mdcomplete").on("click",function(){var n=$(this);$.ajax({type:"POST",url:"/Training/UpdateModuleCompleted",data:{CourseID:$("#CourseID").val(),Serial:$(this).data("mdtext")},success:function(t){if(t==!0){var i=$("#accid").val();$(i).removeClass("fa fa-circle-o").addClass("fa fa-check-circle");$(i).css("background-color","");$(i).css("color","green");n.addClass("hidden")}else $(i).addClass("fa fa-check-circle"),$(i).css("color","orange")},failure:function(){}})});$(document).on("click","#divCoursetools",function(){$.ajax({type:"POST",url:"/Training/GetCoursewareTools",data:'{Courseid:"'+$("#CourseID").val()+'"}',contentType:"application/json;charset=utf-8",dataType:"json",success:function(n){var i,r,t;if(n.length>0){for(i="",r="",t=0;t<n.length;t++)n[t].FileType=="General"&&(i+='<input type="button" class="btn btn-default btn-round btn-block" value="'+n[t].ViewFileName+'" onclick="downloadtools(\''+n[t].Serial+'\')"  style="text-align:left;text-transform: none;font-size:13px" />',$("#idgeneral").show()),n[t].FileType=="Essential"&&(i+='<input type="button" class="btn btn-default btn-round btn-block" value="'+n[t].ViewFileName+'" onclick="downloadtools(\''+n[t].Serial+'\')"  style="text-align:left;text-transform: none;font-size:13px" />',$("#idessential").show(),$("#divessential").show()),n[t].FileType=="Additional"&&(r+='<input type="button" class="btn btn-default btn-round btn-block" value="'+n[t].ViewFileName+'" onclick="downloadtools(\''+n[t].Serial+'\')"  style="text-align:left;text-transform: none;font-size:13px" />',$("#idadditional").show(),$("#divadditional").show());$("#idgeneral").html(i);$("#divessential").html(i);$("#divadditional").html(r);$("#divtools").hide()}else $("#divtools").show()}});window.scrollTo(0,100)})